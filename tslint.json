{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-any": false,
    "ordered-imports": false,
    "semicolon": [true, "always"],
    "member-ordering": [
      true,
      {
          "order": [
            "public-static-field",
            "private-static-field",
            "instance-field",
            "constructor",
            "public-static-method",
            "private-static-method"
          ]
      }
    ]
  }
}
